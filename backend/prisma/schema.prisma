generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.1.x", "debian-openssl-1.1.x", "linux-musl"] 
}


datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model Blog {
  id String @id @default(cuid())
  title String
  content String
  author String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}